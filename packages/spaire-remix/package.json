{
  "name": "@spaire/remix",
  "version": "2.0.0",
  "description": "Spaire integration for Remix and React Router",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "require": "./dist/index.cjs"
    }
  },
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "test": "vitest",
    "build": "tsup ./src/index.ts --format esm,cjs --dts --clean --sourcemap",
    "dev": "tsc --watch",
    "check": "biome check --write ./src"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "spaire",
    "remix",
    "react-router",
    "payments",
    "subscriptions"
  ],
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@sindresorhus/tsconfig": "^7.0.0",
    "@types/node": "^20.0.0",
    "prettier": "^3.7.4",
    "tsup": "^8.5.1",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@spaire/adapter-utils": "^2.0.0",
    "@spaire/sdk": "^0.45.1"
  }
}
